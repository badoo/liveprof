{
  "name": "badoo/liveprof",
  "type": "library",
  "description": "A performance monitoring system for running on live sites",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Badoo Development"
    }
  ],
  "require": {
    "php": ">=5.4",
    "doctrine/dbal": "~2.0|~3.0",
    "psr/log": "~1.0|~2.0",
    "ext-json": "*",
    "ext-zlib": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "psr-4": {
      "Badoo\\LiveProfiler\\": "src/Badoo/LiveProfiler"}
  },
  "autoload-dev": {
    "psr-4": {"unit\\Badoo\\": "tests/unit/Badoo"}
  }
}
